function calculator(a:number , b:number , operator:string) {
    if (operator === '+'){
        return a + b;
    } else if (operator === '-') {
        return a - b;
    } else if (operator === '*') {
        return a * b;
    } else if (operator === '/') {
        return a / b;
    }
}
var a = 15;
var b = 4;

calculator(a,b,'+');
calculator(a,b,'-');
calculator(a,b,'*');
calculator(a,b,'/');

console.log(a + b);
console.log(a - b);
console.log(a * b);
console.log(a / b);
